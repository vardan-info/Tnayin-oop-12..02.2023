----------------------------------------------------Dasaran papka
public class Dasaran {
    private String name;
    private int qanak;

    public Dasaran(String name, int qanak) {
        this.name = name;
        this.qanak = qanak;
    }


    public String getName() {
        return name;
    }

    public int getQanak() {
        return qanak;
    }
        int qanak(){
            return qanak+100;
        }

    }

